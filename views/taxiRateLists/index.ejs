<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                <div class="page-title-box">
                    <h4 class="page-title float-left">Manage Taxi Rate List</h4>
                    <ol class="breadcrumb float-right">
                        <li class="breadcrumb-item"><a href="javascript:void(0);">Taxi Rate List</a></li>
                        <li class="breadcrumb-item active">Index</li>
                    </ol>
                    <div class="clearfix"></div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-12">
                <div class="card-box table-responsive">
                    <div class="col-12 px-3 mb-3 d-flex justify-content-between align-items-center">
                        <h4 class="m-t-0 header-title"><b>Taxi Rate List</b></h4>
                    </div>
                    <table id="datatable-buttons" class="table table-striped table-bordered" cellspacing="0"
                        width="100%">
                        <thead>
                            <tr>
                                <th>Type</th>
                                <th>/KM Charge (INR)</th>
                                <th class="text-center">Action</th>
                            </tr>
                        </thead>
                        <tbody>

                            <% data.forEach((value, key) => { %>

                                <tr>
                                    <td><%= value.type %></td>
                                    <td><%= value.per_km_charge %></td>
                                    <td>
                                        <a href="/admin/taxi_rate_list/edit/<%= value.id %>" class="on-default edit-row pr-1 text-primary" data-toggle="tooltip"
                                        data-placement="top" title="" data-original-title="Edit"><i
                                            class="fa fa-edit"></i></a>
                                    </td>
                                </tr>
    
                            <% }) %>

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>