<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                <div class="page-title-box">
                    <h4 class="page-title float-left">Vendor Activities</h4>
                    <ol class="breadcrumb float-right">
                        <li class="breadcrumb-item"><a href="javascript:void(0);">Vendor Activities</a></li>
                        <li class="breadcrumb-item active">View</li>
                    </ol>
                    <div class="clearfix"></div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="card-box">
                    <h4 class="header-title m-t-0">Rental Activity (<%= data?.activity?.title || '' %>)</h4>
                    <div class="row">
                        <div class="col-sm-3">
                            <div class="text-center card-box">
                                <div class="clearfix"></div>
                                <div class="member-card">
                                    <div class="thumb-xl member-thumb m-b-10 mx-auto">
                                        <img src="<%= data?.user?.image %>" class="rounded-circle img-thumbnail" alt="profile-image">
                                    </div>
                                    <div class="">
                                        <h4 class="m-b-5 font-18"><%= data?.user?.first_name || '' %> <%= data?.user?.last_name || '' %></h4>
                                        <p class="text-muted m-b-0"><span class="text-pink"> <%= data?.user?.email || '' %> </span> </p>
                                        <p class="text-muted"> <%= data?.user?.country_code +' '+ data?.user?.mobile_no || '' %> </p>
                                    </div>
                                    <p class="text-muted font-13"> <%= data?.user?.city || '' %>, <%= data?.user?.state || '' %>, <%= data?.user?.country || '' %> </p>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-9">
                            <h5 class="text-custom m-b-5" style="text-transform: uppercase"> <%= data?.title || '' %> </h5>
                            <span class="label label-primary"><%= data?.brand?.name || '' %></span>
                            <span class="label label-default"><%= data?.model?.name || '' %></span>
                            <span class="label label-info"><%= data?.model?.type || '' %></span>
                            <p class="text-warning"><i class="fa fa-map-marker"></i>
                                <%= data?.user?.vendor_business_detail?.location || '' %>
                            </p>
                            <h4 class="header-title m-t-0">Quantity <span class="label label-success"><%= data?.quantity || '' %></span></h4>
                            <h4 class="header-title m-t-0">Registration Number</h4>
                                
                            <% data?.vehicle_details.forEach((v, k) => { %>

                            <label class="mr-3">
                                <span class="label label-warning"><%= v?.year || '' %></span>
                                <span class="label label-danger"><%= v?.registration_no || '' %></span>
                            </label>

                            <% }) %>
                            
                            <h4 class="header-title m-t-4">Vendor Amount (INR) 
                                <span class="label label-danger"><%= data?.price?.per_hour?.vendor_amount || '' %>/Hour</span>
                                <span class="label label-danger"><%= data?.price?.per_day?.vendor_amount || '' %>/Day</span>
                            </h4>

                            <h4 class="header-title m-t-4">Admin Amount (INR) 
                                <span class="label label-primary"><%= data?.price?.per_hour?.admin_amount || '' %>/Hour</span>
                                <span class="label label-primary"><%= data?.price?.per_day?.admin_amount || '' %>/Day</span>
                            </h4>

                            <h4 class="header-title m-t-4">Price (INR) 
                                <span class="label label-success"><%= data?.price?.per_hour?.amount || '' %>/Hour</span>
                                <span class="label label-success"><%= data?.price?.per_day?.amount || '' %>/Day</span>
                            </h4>
                        </div>
                    </div>
                    <h4 class="header-title m-t-4">What to take</h4>

                    <%if (data?.what_to_take?.length) { %>
                        <% data?.what_to_take.forEach((v, k) => { %>
                            
                            <span class="label label-primary"><%= v?.name || '' %></span>

                        <% }) %>
                    <% } else{ %>
                        <span class="col-sm-3 font-13">N/A</span>
                    <% } %>

                    <h4 class="header-title m-t-4">Things & service included</h4>
                    
                    <%if (data?.thing_service_included?.length) { %>
                        <% data?.thing_service_included.forEach((v, k) => { %>
                            
                            <span class="label label-primary"><%= v?.name || '' %></span>

                        <% }) %>
                    <% } else{ %>
                        <span class="col-sm-3 font-13">N/A</span>
                    <% } %>

                    <h4 class="header-title m-t-4">Addons</h4>

                    <%if (data?.add_ons?.length) { %>
                        <% data?.add_ons.forEach((v, k) => { %>
                            
                            <p class="text-muted m-b-20 font-13"><%= k + 1 %>. <%= v?.item || '' %> <span class="label label-danger"><%= v?.price || '' %> (INR)</span> <span class="label label-primary">Quantity <%= v?.quantity || '' %></span></p>
                            
                        <% }) %>
                    <% } else{ %>
                        <span class="col-sm-3 font-13">N/A</span>
                    <% } %>

                    <h4 class="m-t-4 header-title m-b-4">Activity Images</h4>
                    <div class="row activityImg">

                        <% data?.images.forEach((v, k) => { %>

                            <div class="col-sm-3">
                                <div class="activiBox">
                                    <img src="<%= v?.media_path %>" alt="img" class="img-fluid"/>
                                </div>
                            </div>
    
                        <% }) %>

                    </div>

                    <h4 class="m-t-4 header-title m-b-0">Activity Video</h4>
                    <div class="row">

                        <%if (typeof data?.video?.media_path != 'undefined') { %>

                        <div class="col-sm-3 positon-relative videotop">
                            <a class="popup-vimeo" href="<%= data?.video?.media_path %>">
                                <img src="/assets/images/bg-countdown.jpg" alt="img" class="img-fluid positon-absolute">
                                <span class="videoIcon"> <i class="mdi mdi-play-circle-outline"></i></span>
                            </a>    
                        </div>

                        <% } else{ %>
                            <span class="col-sm-3 font-13">N/A</span>
                        <% } %>
                        
                    </div>
                    
                    <div class="col-lg-12 mt-4">
                        <div class="card-box ribbon-box">
                            <div class="ribbon ribbon-primary">Description</div>
                            <p class="m-b-0"><%= data?.description || '' %></p>
                        </div>
                    </div>
                    <div class="col-lg-12 mt-4">
                        <div class="card-box ribbon-box">
                            <div class="ribbon ribbon-warning">Warning</div>
                            <p class="m-b-0"><%= data?.warning || '' %></p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
